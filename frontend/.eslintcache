assName=\"signUp-H\">\n          <h1>SIGN UP</h1>\n        </div>\n        <div>\n          <input\n            type=\"text\"\n            placeholder=\"User Name\"\n            name=\"name\"\n            onChange={onChange}\n            className=\"inputUser\"\n          />\n        </div>\n\n        <div>\n          <input\n            type=\"Nick Name\"\n            name=\"nickName\"\n            placeholder=\"Nick name\"\n            onChange={onChange}\n            className=\"inputNickname\"\n          ></input>\n        </div>\n        <div>\n          <input\n            type=\"email\"\n            name=\"email\"\n            placeholder=\"Email\"\n            // value={email}\n            onChange={onChange}\n            className=\"inputEmail\"\n          ></input>\n        </div>\n        <div>\n          <input\n            type=\"password\"\n            placeholder=\"Password\"\n            name=\"password\"\n            onChange={onChange}\n            className=\"inputPassword\"\n          ></input>\n        </div>\n\n        <div className=\"checkBox\">\n          <input\n            type=\"checkbox\"\n            required={true}\n            className=\"checkBox_size\"\n          ></input>\n          <label> I agree the K2G Terms</label>\n        </div>\n        <div>\n          <input type=\"submit\" value=\"Sign Up\" className=\"inputRegister\" />\n        </div>\n      </form>\n      {/* </div> */}\n    </div>\n  );\n}\n\nexport default SignUp;\n","/home/max/projects/DCI FINAL/kik2gether/frontend/src/Components/Home/Home.js",["133","134","135"],"import React, { Component } from \"react\";\nimport { Container, Row, Col } from \"reactstrap\";\n\nimport \"./Home.scss\";\n\nexport class Home extends Component {\n  render() {\n    return (\n      <div className=\"homeSection\">\n        <div className=\"homeSection_dotsLeft\"></div>\n        <div className=\"homeSection_dotsRight\"></div>\n        <div className=\"bgGrafic\"></div>\n        {/* <Container className=\"container\">\n          <Row>\n            <Col>12345678</Col>\n          </Row>\n        </Container> */}\n        <div className=\"booking\"></div>\n      </div>\n    );\n  }\n}\n\nexport default Home;\n","/home/max/projects/DCI FINAL/kik2gether/frontend/src/Components/Fields/Fields.js",["136","137","138","139","140"],"/home/max/projects/DCI FINAL/kik2gether/frontend/src/Components/BookingConfirmation/BookingConfirmation.js",[],"/home/max/projects/DCI FINAL/kik2gether/frontend/src/Components/Additional/Additional.js",["141","142","143","144"],"/home/max/projects/DCI FINAL/kik2gether/frontend/src/Components/BookingWrap/BookingWrap.js",["145","146","147","148"],"import React, { useState, Children } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport Fields from \"../Fields\";\nimport Additional from \"../Additional\";\nimport { Route } from \"react-router-dom\";\nimport Payment from \"../Payment\";\nimport BookingConfirmation from \"../BookingConfirmation\";\nimport FieldsMobile from \"../FieldsMobile\";\n\nfunction BookingWrap() {\n  const [booking, setBooking] = useState(false);\n  const [showPayment, setShowPayment] = useState(false);\n\n  console.log(booking);\n  const mql = window.matchMedia(\"(max-width: 800px)\");\n  const mobileView = mql.matches;\n  console.log(\"mobile view is \", mobileView);\n\n  const showLogic = () => {\n    if (!booking && !showPayment) {\n      if (mobileView) {\n        return (\n          <FieldsMobile\n            booking={booking}\n            setBooking={setBooking}\n          ></FieldsMobile>\n        );\n      } else {\n        return <Fields booking={booking} setBooking={setBooking}></Fields>;\n      }\n    } else if (booking && !showPayment) {\n      return (\n        <Additional\n          booking={booking}\n          setBooking={setBooking}\n          setShowPayment={setShowPayment}\n        ></Additional>\n      );\n    } else if (showPayment) {\n      return <Payment booking={bookin